.container {
	width: 100%;
	padding: 20rpx 0;
	box-sizing: border-box;
}

.top-msg {
	width: 100%;
}

.title1,
.title2,
.title3 {
	width: 100%;
	box-sizing: border-box;
	padding: 20rpx 40rpx;
	font-size: 30rpx;
	color: #606266;
	display: flex;
	border-bottom: 1px solid var(--weui-FG-3);
}

.name {
	display: block;
	width: 100rpx;
}

.detail {
	box-sizing: border-box;
	flex: 1;
	display: block;
	padding: 0 20rpx;
	color: #303133;
}

.midele-title {
	position: sticky;
	top: 0;
	left: 0;
	box-sizing: border-box;
	width: 100%;
	height: 100rpx;
	display: flex;
	align-items: center;
	padding: 10rpx 40rpx;
	background-color: #EBEEF5;
	border-bottom: 1px solid var(--weui-FG-3);
	z-index: 99;
	box-sizing: border-box;
}

.labelItem {
	box-sizing: border-box;
	border-bottom: 1px solid var(--weui-FG-3);
	display: flex;
	padding: 30rpx 40rpx;
}

.btn-area {
	box-sizing: border-box;
	width: 100%;
	padding: 20rpx 40rpx;
}

.changeDetail {
	box-sizing: border-box;
	width: 100%;
	padding: 0 40rpx;
	display: flex;
	color: #666666;
	border-bottom: 1px solid var(--weui-FG-3);
}

.changeDetail:first-child {
	border-top: 1px solid var(--weui-FG-3);
}

.detailLeft {
	flex: 1;
	padding: 40rpx 0;
}
.detailmiddle{
	width: 120rpx;
	padding: 40rpx 0;
	color: #909399;
	text-align: center;
}
.detailright {
	flex: 1;
	text-align: right;
	padding: 40rpx 0;
}

.asas{
color: rgb(21, 150, 219);
}

.senceendtitle{
	display: flex;
	justify-content: space-between;
	padding: 0rpx 40rpx;
	margin-top: 15rpx;
}